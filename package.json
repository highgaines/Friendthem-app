{
  "name": "FriendThem",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "test:e2e": "detox test -c ios.sim.debug",
    "test:e2e:build": "detox build",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "storybook": "storybook start -p 7007",
    "lint": "standard --verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "fixcode": "standard --fix",
    "git-hook": "npm run lint -s && npm run test -s",
    "clean:babelrc": "find ./node_modules -name react-packager -prune -o -name '.babelrc' -print | xargs rm -f",
    "postinstall": "npm run clean:babelrc"
  },
  "dependencies": {
    "analytics-react-native": "^1.2.0",
    "apisauce": "^0.14.3",
    "auth0": "^2.9.1",
    "code-push-cli": "^2.1.6",
    "convert-time": "^0.3.0",
    "format-json": "^1.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest-enzyme": "^4.2.0",
    "jest-serializer-enzyme": "^1.0.0",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.0",
    "querystringify": "1.0.0",
    "ramda": "^0.25.0",
    "react": "16.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.0.0",
    "react-icons": "^2.2.7",
    "react-native": "0.50.4",
    "react-native-animatable": "^1.2.4",
    "react-native-auth0": "^1.2.2",
    "react-native-aws3": "0.0.8",
    "react-native-checkbox": "^2.0.0",
    "react-native-communications": "^2.2.1",
    "react-native-config": "^0.10.0",
    "react-native-contacts": "^2.1.1",
    "react-native-expo-image-cache": "^1.1.0",
    "react-native-fast-image": "^2.2.5",
    "react-native-fbsdk": "^0.7.0",
    "react-native-fetch-blob": "^0.10.8",
    "react-native-image-picker": "^0.26.7",
    "react-native-img-cache": "^1.5.3",
    "react-native-lazyload-deux": "^2.0.4",
    "react-native-linear-gradient": "github:react-native-community/react-native-linear-gradient",
    "react-native-modal": "^4.1.1",
    "react-native-modal-selector": "0.0.25",
    "react-native-modalbox": "^1.4.2",
    "react-native-onesignal": "^3.0.9",
    "react-native-permissions": "^1.1.1",
    "react-native-picker": "^4.3.5",
    "react-native-pie": "^0.4.0",
    "react-native-progress": "^3.4.0",
    "react-native-remote-svg": "^1.0.1",
    "react-native-router-flux": "^4.0.0-beta.28",
    "react-native-sensitive-info": "^5.2.0",
    "react-native-sms": "^1.5.1",
    "react-native-svg": "^6.1.3",
    "react-native-svg-image": "^2.0.1",
    "react-native-svg-uri": "^1.2.3",
    "react-native-swipe-list-view": "^1.0.6",
    "react-native-vector-icons": "^4.5.0",
    "react-native-youtube-oauth": "0.0.4",
    "react-navigation": "^1.0.3",
    "react-navigation-redux-helpers": "^1.0.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.5.1",
    "redux-persist": "^4.1.0",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "reduxsauce": "0.7.0",
    "seamless-immutable": "^7.1.3"
  },
  "devDependencies": {
    "@expo/vector-icons": "^6.2.2",
    "@storybook/addon-storyshots": "^3.3.13",
    "@storybook/addons": "^3.3.13",
    "@storybook/channels": "^3.3.13",
    "@storybook/react-native": "^3.3.13",
    "babel-core": "6.26.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-ignite-ignore-reactotron": "^0.3.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-native": "^3.0.2",
    "detox": "^7.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.3.1",
    "husky": "^0.14.3",
    "ignite-ir-boilerplate": "^2.3.0",
    "ignite-redux-persist": "^0.0.7",
    "ignite-standard": "^1.0.0",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.2.0",
    "jest-react-native": "^18.0.0",
    "mocha": "^5.0.1",
    "mockery": "^2.1.0",
    "react-dom": "16.0.0",
    "react-native-elements": "^0.18.5",
    "react-test-renderer": "^16.0.0",
    "reactotron-react-native": "^1.14.0",
    "reactotron-redux": "^1.13.0",
    "reactotron-redux-saga": "^1.13.0",
    "snazzy": "^7.0.0",
    "standard": "10.0.2"
  },
  "jest": {
    "preset": "react-native",
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "unmockedModulePathPatterns": [
      "react",
      "enzyme",
      "jest-enzyme"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/FriendThem.app",
        "build": "xcodebuild -project ios/FriendThem.xcodeproj -scheme FriendThem -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 7"
      }
    }
  },
  "nrpm": {
    "assets": [
      "./assets/Fonts"
    ]
  },
  "config": {},
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "test",
      "jest",
      "expect",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element"
    ]
  }
}
